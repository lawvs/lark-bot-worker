{
  "name": "lark-bot-worker",
  "version": "0.1.0",
  "description": "A Lark/Feishu bot running on Cloudflare Workers",
  "keywords": [
    "lark",
    "feishu",
    "gitlab",
    "webhook",
    "robot",
    "typescript",
    "cloudflare",
    "workers",
    "gitlab",
    "webhook"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:lawvs/lark-bot-worker.git"
  },
  "license": "MIT",
  "author": "lawvs",
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.15.0",
  "main": "dist/worker.js",
  "scripts": {
    "build": "spack --config $INIT_CWD/spack.config.cjs",
    "clean": "rm -rf dist",
    "dev:worker": "node --experimental-fetch dist/worker.js",
    "format": "prettier --check '**/*.{ts,js,json,md}' --ignore-path .gitignore",
    "format:fix": "prettier --write '**/*.{ts,js,json,md}' --ignore-path .gitignore",
    "test": "vitest",
    "typeCheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.100",
    "@types/node": "^20.10.2",
    "gitlab-event-types": "^3.0.1",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  }
}
